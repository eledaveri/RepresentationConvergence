      "source": [
        "def centering_matrix(n, device):\n",
        "    \"\"\"Centering Matrix H = I - 1/n\n",
        "    Args:\n",
        "        n (int): Number of samples\n",
        "        device: Device on wich create the matrix (CPU o GPU)\n",
        "    Returns:\n",
        "        torch.Tensor: Centering Matrix H(n, n)  \n",
        "    \"\"\"\n",
        "    H = torch.eye(n, device=device) - torch.ones((n, n), device=device) / n\n",
        "    return H\n",
        "\n",
        "def linear_hsic(X, Y, device):\n",
        "    \"\"\"Compute Hilbert-Schmidt Independence Criterion (Linear Kernel)\n",
        "        Args:\n",
        "            X (torch.Tensor): Feature Matrix X (n_samples, n_features)\n",
        "            Y (torch.Tensor): Feature Matrix Y (n_samples, n_features)\n",
        "            device: Device on wich create the matrix (CPU o GPU)\n",
        "        Returns:\n",
        "            torch.Tensor: HSIC value    \n",
        "    \"\"\"\n",
        "    n = X.shape[0]\n",
        "    K = torch.matmul(X, X.T) # Gram Matrix X\n",
        "    L = torch.matmul(Y, Y.T) # Gram Matrix Y\n",
        "    H = centering_matrix(n, device)\n",
        "\n",
        "    # Trace(KHLH) / (n-1)^2\n",
        "    KH = torch.matmul(K, H)\n",
        "    LH = torch.matmul(L, H)\n",
        "    return torch.trace(torch.matmul(KH, LH)) / ((n - 1) ** 2)\n",
        "\n",
        "def linear_cka(X, Y):\n",
        "    \"\"\"\n",
        "    Compute the Linear CKA between two feature matrices.\n",
        "    X, Y must have the same number of rows (samples).\n",
        "    Args:\n",
        "        X (torch.Tensor): Feature Matrix X (n_samples, n_features)\n",
        "        Y (torch.Tensor): Feature Matrix Y (n_samples, n_features)\n",
        "    Returns:\n",
        "        torch.Tensor: CKA value \n",
        "    \"\"\"\n",
        "    assert X.shape[0] == Y.shape[0], f\"Error! Different dimensions: {X.shape[0]} vs {Y.shape[0]}\"\n",
        "\n",
        "    hsic_xy = linear_hsic(X, Y, X.device)\n",
        "    hsic_xx = linear_hsic(X, X, X.device)\n",
        "    hsic_yy = linear_hsic(Y, Y, Y.device)\n",
        "\n",
        "    return hsic_xy / (torch.sqrt(hsic_xx) * torch.sqrt(hsic_yy))"
      ]
    },
